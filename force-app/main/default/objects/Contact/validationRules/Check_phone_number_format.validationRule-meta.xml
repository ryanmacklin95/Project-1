<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_phone_number_format</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(OtherPhone, &quot;\\D*?(\\d\\D*?){10}&quot;))</errorConditionFormula>
    <errorDisplayField>OtherPhone</errorDisplayField>
    <errorMessage>Phone numbers should be in this format: (000) 000-0000</errorMessage>
</ValidationRule>
