<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Supplier_Shipping_Street</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
$RecordType.Name = &quot;Supplier Account&quot;,
ISBLANK(ShippingStreet)
)</errorConditionFormula>
    <errorDisplayField>ShippingStreet</errorDisplayField>
    <errorMessage>Must enter a shipping street</errorMessage>
</ValidationRule>
